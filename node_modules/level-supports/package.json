{
  "name": "level-supports",
  "version": "2.0.0",
  "description": "Create a manifest describing the abilities of a levelup or abstract-leveldown db",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && (nyc -s node test/self.js | faucet) && nyc report",
