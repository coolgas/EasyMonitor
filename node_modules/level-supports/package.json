  "files": [
    "test",
    "CHANGELOG.md",
    "CONTRIBUTORS.md",
    "index.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "airtap": "^4.0.3",
    "airtap-playwright": "^1.0.1",
    "coveralls": "^3.0.6",
